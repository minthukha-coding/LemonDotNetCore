@using Newtonsoft.Json
<canvas id="myChart" width="400" height="400"></canvas>

@{
    List<string> _labeldata = Model.Labeldata;
    List<string> _DataSetName = Model.DataSetName;
    List<int> _DataSetData1 = Model.DataSetData;
}

@section scripts {
    <script>

        let labeldata = @(Html.Raw(JsonConvert.SerializeObject(Model.Labeldata)))
        let DataSetName = @(Html.Raw(JsonConvert.SerializeObject(Model.DataSetName)))
        let DataSetData = @(Html.Raw(JsonConvert.SerializeObject(Model.DataSetData)))


        const data = {
            labels: labeldata,
            datasets: [{
                label: DataSetName,
                data: DataSetData,
                fill: false,
                borderColor: 'rgb(75, 192, 192)',
                tension: 0.1
            }]
        };
        const config = {
            type: 'line',
            data: data,
        };
        const myChart = new Chart(document.getElementById('myChart'), config);
    </script>
}